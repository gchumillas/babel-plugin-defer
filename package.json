{
  "name": "babel-plugin-defer",
  "version": "0.4.0",
  "description": "Babel plugin that transpiles defer statements to JavaScript, bringing Go-like defer functionality to JavaScript/TypeScript applications",
  "keywords": [
    "babel",
    "babel-plugin",
    "defer",
    "transpiler",
    "javascript",
    "typescript",
    "go",
    "cleanup",
    "resource-management"
  ],
  "homepage": "https://github.com/your-username/babel-plugin-defer#readme",
  "bugs": {
    "url": "https://github.com/your-username/babel-plugin-defer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/babel-plugin-defer.git"
  },
  "license": "MIT",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "url": "https://your-website.com"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "node scripts/clean.js",
    "build": "npm run clean && rollup -c",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@eslint/eslintrc": "^3.3.1",
    "@rollup/plugin-typescript": "^12.1.3",
    "@stylistic/eslint-plugin": "^5.0.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__traverse": "^7.20.7",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "rollup": "^4.44.0",
    "rollup-plugin-dts": "^6.2.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.35.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.23.0"
  },
  "dependencies": {},
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/your-username"
  }
}
